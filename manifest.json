{
  "name": "网页无障碍反馈",
  "version": "0.1",
  "description": "为用户提供一种反馈网页无障碍问题的方式",
  "background": {
    "persistent": true,
    "scripts": ["jquery-2.0.0.min.js","background.js"]
  },
  "browser_action": {
    "default_icon": "accessible.png",
    "default_title": "截图反馈问题"
  },
  "permissions": [
    "<all_urls>"
  ],
  "manifest_version": 2
}
